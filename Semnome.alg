algoritmo "semnome"
var
valorA, valorB, valorC : inteiro
inicio

escreval("Informe 3 numeros")
escreval("Informe o valor de A")
leia(valorA)
escreval("Informe o valor de B")
leia(valorB)
escreval("Informe o valor de C")
leia(valorC)
SE(valorA + valorB) > (valorC)entao
escreval("A soma de ",valorA," e ",valorB," é maior que ",valorC)
FIMSE
fimalgoritmo
